            "user_size_y": 380.0,
            "id": 12,
            "type_id": 4,
            "source_code": "int prev = -(2000/60);\nint prev2 = prev*2;\n\nvoid step()\n{\n  float x,y,z,x1,y1,z1,x2,y2,z2;\n  x =  Read(0,0);     y = Read(1,0);      z = Read(2,0);\n  x1 = Read(0,prev);  y1 = Read(1,prev);  z1 = Read(2,prev);\n  x2 = Read(0,prev2); y2 = Read(1,prev2); z2 = Read(2,prev2);\n\n  // ------ Position ------\n  Write(0, x*10000*60);\n  Write(1, y*10000*60);\n  Write(2, z*10000*60);\n\n  // ------ Velocity ------\n  float veloX, veloY, veloZ, veloX1, veloY1, veloZ1;\n  veloX  = (x-x1);   veloY  = (y-y1);  veloZ = (z-z1);\n  veloX1 = (x1-x2);  veloY1 = (y1-y2); veloZ = (z1-z2);\n  Write(3, veloX * 60);\n  Write(4, veloY * 60);\n  Write(5, veloZ * 60);\n\n  // ------ Acceleration ------\n  Write(6, (veloX - veloX1) * 60);\n  Write(7, (veloY - veloY1) * 60);\n  Write(8, (veloZ - veloZ1) * 60);\n}"
